{
  "name": "@delgadotrueba/http",
  "version": "1.0.4",
  "description": "A personal HTTP service library featuring both httpService for real-world API interactions and httpMockService for testing and mock API simulations.",
  "author": "delgadotrueba",
  "keywords": [
    "http",
    "httpMock",
    "httpMockService",
    "httpService"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DelgadoTrueba/http"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:mjs",
    "build:cjs": "npx tsc -p tsconfig.cjs.json && node scripts/rename-files.js cjs",
    "build:mjs": "npx tsc -p tsconfig.mjs.json && node scripts/rename-files.js mjs",
    "lint": "npx eslint  -c .eslintrc.json src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:cjs && npm run test:mjs",
    "test:cjs": "npx jest -c jest.cjs.config.js --verbose",
    "test:mjs": "npx jest -c jest.mjs.config.js --verbose",
    "test:coverage": "npm run test -- --coverage",
    "prepare": "husky",
    "prepublishOnly": "npm run test && npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@types/jest": "29.5.12",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "commitlint": "19.3.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "29.7.0",
    "lint-staged": "15.2.8",
    "prettier": "3.3.3",
    "ts-jest": "29.2.4",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "path-to-regexp": "6.2.1",
    "tslib": "2.6.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/src/index.cjs.js",
  "module": "./dist/src/index.mjs.js",
  "types": "./dist/src/index.mjs.d.ts",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/src/index.cjs.js",
        "types": "./dist/src/index.cjs.d.ts"
      },
      "import": {
        "default": "./dist/src/index.mjs.js",
        "types": "./dist/src/index.mjs.d.ts"
      }
    },
    "./public-api": {
      "require": {
        "default": "./dist/src/public-api.cjs.js",
        "types": "./dist/src/public-api.cjs.d.ts"
      },
      "import": {
        "default": "./dist/src/public-api.mjs.js",
        "types": "./dist/src/public-api.mjs.d.ts"
      }
    }
  },
  "sideEffects": false
}
